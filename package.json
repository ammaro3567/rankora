{
  "name": "rankora",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "AI-Powered Content Analysis for Google's AI Overviews",
  "keywords": [
    "ai",
    "content-analysis",
    "seo",
    "google-ai-overviews",
    "react",
    "typescript",
    "saas"
  ],
  "author": "Rankora Team",
  "license": "MIT",
  "homepage": "https://rankora.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/rankora.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/rankora/issues"
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run gen-icons && vite build && npm run copy-manifest && npm run generate-sitemap",
    "gen-icons": "node scripts/generate-icons.mjs",
    "copy-manifest": "cp public/site.webmanifest dist/ || copy public\\site.webmanifest dist\\",
    "build:prod": "npm run type-check && npm run lint && vite build --mode production",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "predeploy": "npm run build:prod",
    "deploy": "gh-pages -d dist",
    "generate-sitemap": "echo 'Sitemap will be generated by Netlify plugin'",
    "analyze": "npx vite-bundle-analyzer dist/stats.html",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@clerk/clerk-react": "^5.42.0",
    "@paypal/react-paypal-js": "^8.8.3",
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/supabase-js": "^2.53.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.8.0",
    "recharts": "^3.1.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "gh-pages": "^6.1.1",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "sharp": "^0.34.3",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
